<div class="header bg-primary pb-6">
    <div class="container-fluid">
      <div class="header-body">
        <div class="row align-items-center py-4">
          <div class="col-lg-6 col-7">
            <h6 class="h2 text-white d-inline-block mb-0">Application</h6>
            <nav aria-label="breadcrumb" class="d-none d-md-inline-block ml-md-4">
              <ol class="breadcrumb breadcrumb-links breadcrumb-dark">
                <li class="breadcrumb-item"><a href="/"><i class="fas fa-home"></i></a></li>
                <li class="breadcrumb-item"><a href="/applications">Applications</a></li>
                <li class="breadcrumb-item active" aria-current="page"><%= appId %></li>
              </ol>
            </nav>
          </div>
          <div class="col-lg-6 col-5 text-right">
            <!-- <a href="application/add-application" class="btn btn-sm btn-neutral">Add Application</a> -->
            <!-- <a href="#" class="btn btn-sm btn-neutral">Filters</a> -->

            <!-- <a class="btn btn-neutral" href="#">
              <i class="fas fa-wifi text-primary"></i>
              <span class="nav-link-text">Subscribe</span>
            </a> -->
            <a class="btn btn-neutral" href="/applications/<%= appId %>/useraccess">
              <i class="fas fa-users text-primary"></i>
              <span class="nav-link-text">User Access</span>
            </a>
            <a class="btn btn-neutral" href="/applications/<%= appId %>/add-device">
              <i class="fas fa-plus text-primary"></i>
              <span class="nav-link-text">Add Device</span>
            </a>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- Page content -->
  <div class="container-fluid mt--6">
    <div class="row justify-content-center">
      <div class=" col ">
        <div class="card">
          <div class="row">
            <div class="card-header bg-transparent">
              <h2 class="mb-0 ml-4"><%= appId %> Devices</h2>
            </div>
            <div class="row ml-3 mt-3 mb-3">
              <form class="navbar-search navbar-search-light form-inline mr-sm-3" id="navbar-search-main">
                <div class="form-group mb-0">
                  <div class="input-group input-group-alternative input-group-merge">
                    <div class="input-group-prepend">
                      <span class="input-group-text"><i class="fas fa-search"></i></span>
                    </div>
                    <input class="form-control" placeholder="Search" type="text">
                  </div>
                </div>
                <button type="button" class="close" data-action="search-close" data-target="#navbar-search-main" aria-label="Close">
                  <span aria-hidden="true">Ã—</span>
                </button>
              </form>
              <a href="#" class="btn btn-neutral">
                <i class="fas fa-filter"></i>
                <span>Filter</span>
              </a>
            </div>
          </div>
          <div class="card-body">
            <div class="row icon-examples">
              <% for(var i=0; i < datas.length; i++) { %>
                <a href="/applications/<%= datas[i].appId %>/device/<%= datas[i].deviceName %>" class= "col-lg-3 col-md-6 mb-3 mr-3 ml-3 btn btn-neutral text-left">
                  <div class ="card-body">
                    <span><%= datas[i].appId %></span>
                    <span><%= datas[i].deviceName %></span>
                    <span><%= datas[i].userId %></span>
                  </div>
              </a>
          <% } %>
             
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>